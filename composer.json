{
    "name": "smoobu/laminas-services-scanner-cli",
    "description": "CLI tool to inspect and display all registered services in a Laminas container",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patryk Cwiklinski",
            "email": "patryk.cwiklinski.ext@smoobu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.0|^7.0",
        "laminas/laminas-servicemanager": "^3.0",
        "laminas/laminas-config": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AiSupaScan\\LaminasServicesCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AiSupaScan\\LaminasServicesCli\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/laminas-services"
    ],
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src --level=8"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
